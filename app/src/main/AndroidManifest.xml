<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="theunderground.com.ucrmap">

    <!-- Permissions we are going to work with put below-->

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--All Activities go here-->
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <!-- <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter> -->
        </activity>
        <activity
            android:name=".ForgotLogin"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.LoginActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.LoginActivity" />
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.LoginActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.LoginActivity" />
        </activity>
        <activity
            android:name=".AccountActivity"
            android:label="@string/app_name"

            android:parentActivityName="theunderground.com.ucrmap.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.LoginActivity" />
        </activity>
        <activity
            android:name=".ILearnActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.AccountActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.AccountActivity" />
        </activity>
        <activity
            android:name=".LibraryActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.AccountActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.AccountActivity" />
        </activity>
        <activity
            android:name=".UCRMapActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.AccountActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.AccountActivity" />
        </activity>

        <activity
            android:name=".CampusSafetyActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.AccountActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.AccountActivity" />
        </activity>
        <activity
            android:name=".ContactUsActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.AccountActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.AccountActivity" />
        </activity>
        <activity
            android:name=".DinningHallActivity"
            android:label="@string/app_name"
            android:parentActivityName="theunderground.com.ucrmap.AccountActivity">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="theunderground.com.ucrmap.AccountActivity" />
        </activity>

    </application>

</manifest>
